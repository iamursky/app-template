{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "ios": "cross-env NODE_ENV=development expo run:ios --device",
    "android": "cross-env NODE_ENV=development expo run:android --device",
    "web": "cross-env NODE_ENV=development expo start --web -c",
    "build:ios": "cross-env NODE_ENV=production eas build --local -p ios -e production",
    "build:android": "cross-env NODE_ENV=production eas build --local -p android -e production",
    "build:web": "cross-env NODE_ENV=production expo export -p web",
    "deploy:web": "cross-env NODE_ENV=production eas deploy --prod --non-interactive",
    "pull-env": "eas env:pull --non-interactive --environment=production",
    "format": "prettier --write src && eslint --fix src",
    "lint": "tsc --noemit && eslint src"
  },
  "dependencies": {
    "@expo-google-fonts/noto-sans": "0.3.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/analytics": "21.13.0",
    "@react-native-firebase/app-check": "21.13.0",
    "@react-native-firebase/app": "21.13.0",
    "@react-native-firebase/crashlytics": "21.13.0",
    "@react-native-firebase/perf": "21.13.0",
    "clsx": "2.1.1",
    "expo-build-properties": "0.13.2",
    "expo-constants": "17.0.8",
    "expo-dev-client": "5.0.20",
    "expo-font": "13.0.4",
    "expo-linking": "7.0.5",
    "expo-router": "4.0.20",
    "expo-splash-screen": "0.29.22",
    "expo-status-bar": "2.0.1",
    "expo": "52.0.44",
    "firebase-admin": "13.2.0",
    "firebase": "11.6.0",
    "react-dom": "18.3.1",
    "react-native-reanimated": "3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.4.0",
    "react-native-web": "0.19.13",
    "react-native": "0.76.9",
    "react": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@eslint/js": "9.24.0",
    "@types/react-dom": "18.3.1",
    "@types/react-native-web": "0.19.0",
    "@types/react": "18.3.12",
    "cross-env": "7.0.3",
    "eas-cli": "16.3.1",
    "eslint-config-expo": "8.0.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-perfectionist": "4.11.0",
    "eslint": "9.24.0",
    "nativewind": "4.1.23",
    "prettier-plugin-tailwindcss": "0.6.11",
    "prettier": "3.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1"
  }
}
